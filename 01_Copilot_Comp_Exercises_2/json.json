[
    {
        "actions": [
            "s3:GetObject"
        ],
        "condition": null,
        "effect": "Allow",
        "resources": [
            "arn:aws:s3:::mybucket/*"
        ]
    },
    {
        "actions": [
            "ec2:StartInstances"
        ],
        "condition": {
            "StringEquals": {
                "aws:RequestedRegion": "us-west-2"
            }
        },
        "effect": "Deny",
        "resources": [
            "*"
        ]
    }
]