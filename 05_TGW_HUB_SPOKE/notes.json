                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents",
                "logs:DescribeLogGroups",
                "logs:DescribeLogStreams"
            ],
            "Effect": "Allow",
            "Resource": [
                "*"
            ]
        },
        {
            "Action": [
                "s3:DeleteBucket"
            ],
            "Effect": "Deny",
            "Resource": [
                "*"
            ]
        },
        {
            "Action": [
                "s3:DeleteBucket"
            ],
            "Condition": {
                "Bool": {
                    "aws:MultiFactorAuthPresent": "true"
                }
            },
            "Effect": "Deny",
            "Resource": [
                "*"
            ]
        }
    ],
    "Version": "2012-10-17"
}

